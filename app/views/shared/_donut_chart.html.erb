<%= donut_chart data,
  class: 'box',
  height: "250px",
  width: "600px",
  options: {
    backgroundColor: '#0A4D68',
    legend: {
      orient: 'vertical', right: 'right',
      height: "250px",
      width: "250px",
      backgroundColor: '#0A4D68',
      textStyle: {
        color: "#C4C4C5",

      }
    },
    emphasis: {
      itemStyle: {
        shadowBlur: 10,
        shadowOffsetX: 0,
        shadowColor: 'rgba(0, 0, 0, 0.5)'
      }
    }
  }
%>
